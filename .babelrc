{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties"
    ],
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2, // polyfill 需要使用@babel/runtime-corejs2
        "useBuildIns": "usage"//按需引入,即使用什么新特性打包什么新特性, 可以减小打包的体积
      }
    ],
    [
      "babel-plugin-import",
      {
        "libraryName": "antd",
        "style": "css"
      },
      "import-antd"
    ],
    [
      "babel-plugin-import",
      {
        "libraryName": "lodash",
        "libraryDirectory": "",
        "camel2DashComponentName": false
      },
      "import-lodash"
    ]
  ]
}